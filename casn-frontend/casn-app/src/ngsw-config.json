{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],
  "dataGroups": [
      {
        "name": "api-freshness",
        "urls": ["https://test.casn.app/api/appointments/**"],
        "cacheConfig": {
          "maxSize": 3,
          "maxAge": "1d",
          "strategy": "freshness"
        }
      },
      {
        "name": "api-performance",
        "urls": [
          "https://test.casn.app/api/serviceprovider/**",
          "https://test.casn.app/api/appointmentType/**",
          "https://test.casn.app/api/driveCancelReason/**",
          "https://test.casn.app/api/driveStatus/**",
          "https://test.casn.app/appconfig.json"
        ],
        "cacheConfig": {
          "maxSize": 3,
          "maxAge": "1d",
          "strategy": "performance"
        }
      }
    ]
}
