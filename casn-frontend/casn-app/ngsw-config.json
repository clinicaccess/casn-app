{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-freshness",
      "urls": ["https://casnapptest.dmwilson.info/api/appointments/**"],
      "cacheConfig": {
        "maxSize": 3,
        "maxAge": "1d",
        "strategy": "freshness"
      }
    },
    {
      "name": "api-performance",
      "urls": [
        "https://casnapptest.dmwilson.info/api/clinic/**",
        "https://casnapptest.dmwilson.info/appconfig.json"
      ],
      "cacheConfig": {
        "maxSize": 3,
        "maxAge": "1d",
        "strategy": "performance"
      }
    }
  ]
}
